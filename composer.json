{
    "name": "composer starter pack",
    "description": "Installing WordPress in a subdirectory with Composer etc",
    "version": "1.0",
    "keywords": [,"wordpress","deployment"],
    "homepage": "http://AvalaraDev.com",
    "license": "MIT",
    "authors": [
        {
            "name": "AvalaraDev",
            "email": "name.last@avalara.com",
            "homepage": "name.last@avalara.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "git",
            "url": "https://github.com/AvalaraDev/test-MtgExciseMicrositeTheme.git"
        },
        {
            "type": "git",
            "url": "https://github.com/AvalaraDev/test-MtgCertcaptureMicroTheme.git"
        },
        {
            "type": "git",
            "url": "https://github.com/AvalaraDev/test-MtgExciseMicrositePlugin.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "*",
        "johnpbloch/wordpress": "*",
        "roots/wp-stage-switcher": "2.0.0",
        "horiondigital/advanced-custom-fields-front-end-editor": "dev-master",
        "trendwerk/redirects": "1.0.6",
        "moxie-lean/wp-gravity-forms": "0.1.5",
        "horiondigital/advanced-custom-fields-front-end-editor": "dev-master",
        "AvalaraDev/test-MtgExciseMicrositeTheme": "dev-master",
        "AvalaraDev/test-MtgCertcaptureMicroTheme": "dev-master",
        "AvalaraDev/test-MtgExciseMicrositePlugin": "dev-master"
    },
    "extra": {
    	"wordpress-install-dir": "wp",
      "installer-paths": {
            "public/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "public/wp-content/themes/{$name}/": ["type:wordpress-theme"]

    }
    }
}

